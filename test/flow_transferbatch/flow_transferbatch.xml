          <item name="1. end item"/>
          <quantity>1</quantity>
        </flow>
        <flow xsi:type="flow_transfer_batch">
          <item name="1. subassembly"/>
          <quantity>-1</quantity>
          <transferbatch>100</transferbatch>
        </flow>
      </flows>
      <loads>
        <load>
          <resource name="1. assembly line"/>
